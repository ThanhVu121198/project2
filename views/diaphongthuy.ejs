                <br>
            </div>
            <div class="abs_mobie">
                <select>
                    <option>default sort</option>
                    <option>from a to z</option>
                    <option>prices gradually</option>
                    <option>prices increase</option>
                    <option>Latest</option>
                </select>
                <hr>
            </div>
            <!--      đây là phần content -->
            <div class="box_content">
                <% for (var i=0;i<SanPham.length;i++){ %>
                    <div class="box_column">
                        <a href="/chi-tiet-sp?id=<%= SanPham[i].MaSP %>"><div class="image_box">
                                <figure><img src="<%= SanPham[i].Image %>" width="299" height="300"></figure>
                                <div class="name_product">
                                    <h5>EGO WEAR</h5>
                                    <h4><%= SanPham[i].TenSP %></h4>
                                    <div class="gia">
                                        399.000.000đ
                                    </div>
                                    <div class="gia2">
                                        460.000.000đ
                                    </div>
                                </div>
                                <button class="heat">
                                    <i class="far fa-heart"></i>
                                </button>
                                <button class="buy">
                                    <i class="fas fa-shopping-basket"> +1</i>
                                </button>
                                <button class="buy_now"><a href="/chi-tiet-sp?id=<%= SanPham[i].MaSP %>">
                                        <i class="fas fa-shopping-basket"> Buy Now</i>
                                    </a></button>
                            </div></a>
                    </div>
                <% } %>

                </div>
            </div>
